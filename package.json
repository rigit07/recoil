{
  "name": "recoil",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.0.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.3",
    "recoil-relay": "^0.1.0",
    "relay-compiler": "^14.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run relay && react-scripts start",
    "build": "npm run relay && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler"
  },
  "relay": {
    "src": "./src",
    "schema": "./src/schema/schema.graphql",
    "language": "javascript",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^14.0.0",
    "graphql": "^16.5.0"
  }
}
